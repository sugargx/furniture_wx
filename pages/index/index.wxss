/* first */

page {
  height: 100%;
  width: 100%;
}

scroll-view {
  height: 100%;
  width: 100%;
}

.first_info {
  text-align: center;
  display: flex;
  flex-direction: column;
  align-items: center;
  justify-content: center;
  font-size: 50rpx;
  padding-left: 10rpx;
  padding-right: 10rpx;
}

.first_info .one {
  margin-top: -20rpx;
  padding-bottom: 20rpx;
}

.first_info span {
  display: inline;
  color: red;
}

.first {
  width: 100%;
  height: 90%;
  position: relative;
}

.first .background {
  width: 100%;
  height: 100%;
  display: block;
}

.first button {
  /* height: 10%; */
  width: 100%;
  display: block;
  padding: 0;
  border: none;
  margin-top: -10rpx;
}

.first .content {
  position: absolute;
  height: 100%;
  width: 100%;
  /* top: 9%; */
  top:5%;
}

.content .chengnuo {
  text-align: center;
}

/* 表单 */

.biaodan {
  height: 750rpx;
  /* text-align: center; */
}

.biaodan .single {
  margin: 30rpx;
}

.consault image {
  height: 150rpx;
  width: 100%;
  position: fixed;
  bottom: 0px;
  z-index: 2;
}

/* 下拉框 */

.select_box {
  background: #fff;
  width: 690rpx;
  height: 88rpx;
  margin: 30rpx auto;
  position: relative;
  margin-top: 1.17647059em;
  background-color: #fff;
  /* line-height: 1.41176471; *//* font-size: 17px; */
}

.select {
  box-sizing: border-box;
  width: 100%;
  height: 70rpx;
  /* border: 1px solid #efefef; */
  border-radius: 8rpx;
  display: flex;
  align-items: center;
  /* padding: 0 20rpx; */
  padding: 10px 15px;
  position: relative;
}

.select_text {
  font-size: 34rpx;
  flex: 1;
  color: #888;
}

.select_img {
  width: 40rpx;
  height: 40rpx;
  display: block;
  transition: transform 0.3s;
}

.select_img_rotate {
  transform: rotate(180deg);
}

.option_box {
  position: absolute;
  top: 70rpx;
  width: 100%;
  /* border: 1px solid #efefef; */
  box-sizing: border-box;
  height: 0;
  overflow-y: auto;
  border-top: 0;
  background: #fff;
  transition: height 0.3s;
  z-index: 9999;
}

.option {
  display: block;
  line-height: 40rpx;
  font-size: 34rpx;
  border-bottom: 1px solid #efefef;
  padding: 10rpx;
}

